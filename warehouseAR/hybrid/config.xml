<?xml version='1.0' encoding='utf-8'?>
<widget id="com.oracle.lad.demo.monopoly" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Warehouse AR</name>
    <description>
        A sample Oracle JavaScript Extension Toolkit (JET) mobile app based on Cordova
    </description>
    <author email="undefined" href="http://www.oraclejet.org">
        Hernan Aymard
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="windows">
        <preference name="windows-target-version" value="10.0" />
        <icon height="30" src="res/icon/windows/Square30x30Logo.scale-100.png" width="30" />
        <icon height="44" src="res/icon/windows/Square44x44Logo.scale-100.png" width="44" />
        <icon height="106" src="res/icon/windows/Square44x44Logo.scale-240.png" width="106" />
        <icon height="70" src="res/icon/windows/Square70x70Logo.scale-100.png" width="70" />
        <icon height="71" src="res/icon/windows/Square71x71Logo.scale-100.png" width="71" />
        <icon height="170" src="res/icon/windows/Square71x71Logo.scale-240.png" width="170" />
        <icon height="150" src="res/icon/windows/Square150x150Logo.scale-100.png" width="150" />
        <icon height="360" src="res/icon/windows/Square150x150Logo.scale-240.png" width="360" />
        <icon height="310" src="res/icon/windows/Square310x310Logo.scale-100.png" width="310" />
        <icon height="150" src="res/icon/windows/Wide310x150Logo.scale-100.png" width="310" />
        <icon height="360" src="res/icon/windows/Wide310x150Logo.scale-240.png" width="744" />
        <splash height="300" src="res/screen/windows/SplashScreen.scale-100.png" width="620" />
        <splash height="1920" src="res/screen/windows/SplashScreenPhone.scale-240.png" width="1152" />
    </platform>
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="DisallowOverscroll" value="true" />
        <icon height="36" src="res/icon/android/icon-ldpi.png" width="36" />
        <icon height="48" src="res/icon/android/icon-mdpi.png" width="48" />
        <icon height="72" src="res/icon/android/icon-hdpi.png" width="72" />
        <icon height="96" src="res/icon/android/icon-xhdpi.png" width="96" />
        <icon height="144" src="res/icon/android/icon-xxhdpi.png" width="144" />
        <icon height="192" src="res/icon/android/icon-xxxhdpi.png" width="192" />
        <splash density="land-ldpi" src="res/screen/android/splash-land-ldpi.9.png" />
        <splash density="land-mdpi" src="res/screen/android/splash-land-mdpi.9.png" />
        <splash density="land-hdpi" src="res/screen/android/splash-land-hdpi.9.png" />
        <splash density="land-xhdpi" src="res/screen/android/splash-land-xhdpi.9.png" />
        <splash density="land-xxhdpi" src="res/screen/android/splash-land-xxhdpi.9.png" />
        <splash density="land-xxxhdpi" src="res/screen/android/splash-land-xxxhdpi.9.png" />
        <splash density="port-hdpi" src="res/screen/android/splash-port-hdpi.9.png" />
        <splash density="port-ldpi" src="res/screen/android/splash-port-ldpi.9.png" />
        <splash density="port-mdpi" src="res/screen/android/splash-port-mdpi.9.png" />
        <splash density="port-xhdpi" src="res/screen/android/splash-port-xhdpi.9.png" />
        <splash density="port-xxhdpi" src="res/screen/android/splash-port-xxhdpi.9.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/splash-port-xxxhdpi.9.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="Orientation" value="all" />
        <preference name="DisallowOverscroll" value="true" />
        <icon height="29" src="res/icon/ios/icon-small.png" width="29" />
        <icon height="58" src="res/icon/ios/icon-small@2x.png" width="58" />
        <icon height="87" src="res/icon/ios/icon-small@3x.png" width="87" />
        <icon height="40" src="res/icon/ios/icon-40.png" width="40" />
        <icon height="80" src="res/icon/ios/icon-40@2x.png" width="80" />
        <icon height="120" src="res/icon/ios/icon-40@3x.png" width="120" />
        <icon height="50" src="res/icon/ios/icon-50.png" width="50" />
        <icon height="100" src="res/icon/ios/icon-50@2x.png" width="100" />
        <icon height="57" src="res/icon/ios/icon.png" width="57" />
        <icon height="114" src="res/icon/ios/icon@2x.png" width="114" />
        <icon height="72" src="res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="res/icon/ios/icon-72@2x.png" width="144" />
        <icon height="76" src="res/icon/ios/icon-76.png" width="76" />
        <icon height="152" src="res/icon/ios/icon-76@2x.png" width="152" />
        <icon height="167" src="res/icon/ios/icon-83.5@2x.png" width="167" />
        <icon height="60" src="res/icon/ios/icon-60.png" width="60" />
        <icon height="120" src="res/icon/ios/icon-60@2x.png" width="120" />
        <icon height="180" src="res/icon/ios/icon-60@3x.png" width="180" />
        <splash height="960" src="res/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="1136" src="res/screen/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/screen/ios/Default-667h.png" width="750" />
        <splash height="2208" src="res/screen/ios/Default-736h.png" width="1242" />
        <splash height="480" src="res/screen/ios/Default~iphone.png" width="320" />
        <splash height="768" src="res/screen/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="1242" src="res/screen/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="2048" src="res/screen/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="1024" src="res/screen/ios/Default-Portrait~ipad.png" width="768" />
    </platform>
    <hook src="scripts/hooks/jetAfterPrepare.js" type="after_prepare" />
    <plugin name="com.wikitude.phonegap.WikitudePlugin" spec="\demos\wikitude-cordova-plugin-7.0.0-3.5.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
</widget>
